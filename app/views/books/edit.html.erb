

  <h1>Editing Book</h1>
 <%= form_with model: @book do |f| %>
  <nav class="edit">Title</nav>
  <%= f.text_field :title %>

  <nav class="edit">Body</nav>
  <%= f.text_area :body %>
  <br>
  <h5><%= f.submit 'Update book' %><h5>

    <%= link_to "Show", book_path(@book.id)%>
    <%= link_to "Back", "/books" %>


<% end %>